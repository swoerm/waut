<UserControl x:Class="Waut.Configurator.Views.ControlModuleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Waut.Configurator.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="800">

    <UserControl.DataContext>
        <local:ControlModuleViewModel/>
    </UserControl.DataContext>

    <Grid>
         
        <DataGrid Name="CMGrid" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="300" DataContext="{Binding cm}"/>

        <Button 
             Content="Import File" 
             Command="{Binding ImportFileCommand}"  
             Width="100" Height="100" Margin="0,10,0,349"/>


    </Grid>

</UserControl>
